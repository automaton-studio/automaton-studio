{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppConfig": {
    "IsDesktop": true
  },
  "ApiConfig": {
    "BaseUrl": "https://localhost:7091/",
    "FlowsUrl": "api/flows",
    "RunnersUrl": "api/runners",
    "LogsUrl": "api/logs"
  },
  "AuthenticationConfig": {
    "RegisterUserUrl": "api/account/registeruser",
    "UpdateUserProfileUrl": "api/account/updateuserprofile",
    "LoginUserUrl": "api/account/loginuser",
    "RefreshAccessTokenUrl": "api/token/refreshaccesstoken",
    "RefreshTokenExpirationMinutesCheck": 2
  },
  "OptionalConfig": {
    "UserSignUp": true
  }
}
