{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppConfiguration": {
    "WebApiUrl": "https://localhost:7091/",
    "FlowsUrl": "api/flows",
    "IsDesktop": false
  },
  "AuthConfiguration": {
    "WebApiUrl": "https://localhost:7091/",
    "LoginUserUrl": "api/account/loginuser",
    "RefreshAccessTokenUrl": "api/token/refreshaccesstoken",
    "RefreshTokenExpirationMinutesCheck": 2
  }
}
