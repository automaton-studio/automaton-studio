@using Automaton.Studio.Steps

<StepDesigner Step=@Step>
    <div>
        <b>@Step.DisplayName</b>
    </div>
    <div>
        Log <a href="" @onclick:preventDefault>@Step.Expression</a> to console
    </div>
</StepDesigner>

@code {
    [Parameter]
    public TestAssertStep Step { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
    }
}

